[color]
    diff = auto
    status = auto
    branch = auto
    grep = auto
[core]
    excludesfile = /Users/james/.gitignore
[alias]
    a = add
    br = branch
    cdiff = log -p -1 --stat
    ci = commit
    co = checkout
    cp = cherry-pick
    cpp = !sh -c "git log -p -1 $1 "
    d = diff
    fixup = commit --amend --no-edit
    gr = grep --color -p -n --full-name
    lg = log --graph --pretty=format:'%Cred%h%Creset %G? -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
    mg = merge --no-ff --no-edit
    p = log -1 -p
    st = status
    u = checkout --
[user]
	name = James Socol
	email = James_Socol@condenast.com
[github]
    username = jsocol
[push]
	default = upstream
[pull]
	rebase = true
