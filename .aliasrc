alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias l.='ls -d .* --color=auto'
alias lh='ls -lh --color=auto'
alias ll='ls -l --color=auto'
alias ls='ls --color=auto'
alias check='check.py'
alias df='df -h'
alias diff='diff -u'
alias django='python manage.py'
alias edconf='sudo vim /etc/httpd/conf/httpd.conf'
