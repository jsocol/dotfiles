ls --color=auth >/dev/null 2>&1
LINUX=$?
if [ $LINUX -eq 0 ]; then
    alias l.='ls -d .* --color=auto'
    alias lh='ls -lh --color=auto'
    alias ll='ls -l --color=auto'
    alias ls='ls --color=auto'
else
    alias l.='ls -d .* -G'
    alias lh='ls -lh -G'
    alias ll='ls -l -G'
    alias ls='ls -G'
fi
alias ipython3='~/.virtualenvs/py3k/bin/ipython'
alias gulp='./node_modules/.bin/gulp'
